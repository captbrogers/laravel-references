{
    "name": "jameslkingsley/laravel-references",
    "description": "Drop-in model references.",
    "homepage": "https://github.com/jameslkingsley/laravel-references",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "illuminate/support": "~5.5.0",
        "illuminate/database": "~5.5.0",
        "illuminate/http": "~5.5.0",
        "illuminate/routing": "~5.5.0",
        "spatie/macroable": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Kingsley\\References\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kingsley\\References\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kingsley\\References\\ReferencesServiceProvider"
            ]
        }
    }
}
